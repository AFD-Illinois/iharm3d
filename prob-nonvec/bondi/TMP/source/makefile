CC = /usr/lib64/mpich/bin/mpicc

CCFLAGS = -std=c99 -mcmodel=medium -fopenmp -march=native -Wall -Ofast -flto -funroll-loops  -L/usr/local/fake_hdf5/lib/  -I/usr/local/fake_hdf5/include/
