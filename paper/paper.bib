	keywords = {Magnetohydrodynamics, C++ languages, Computer architecture, D.2.8.b performance measures, D.3.2.d concurrent, distributed and parallel languages, Graphics processing units, I.6.8.h parallel, J.2.c astronomy, J.2.i physics, Kernel, Message systems, Programming},
	pages = {85--97},
	file = {IEEE Xplore Abstract Record:/var/home/bprather/Zotero/storage/QQX4728Y/9143480.html:text/html;Submitted Version:/var/home/bprather/Zotero/storage/JBARII8U/Grete et al. - 2021 - K-Athena A Performance Portable Structured Grid F.pdf:application/pdf},
