@article{sadowski_semi-implicit_2013,
	title = {Semi-implicit scheme for treating radiation under {M1} closure in general relativistic conservative fluid dynamics codes},
	volume = {429},
	url = {https://ui.adsabs.harvard.edu/abs/2013MNRAS.429.3533S/abstract},
	doi = {10.1093/mnras/sts632},
	abstract = {A numerical scheme is described for including radiation in multidimensional general relativistic conservative fluid dynamics codes. In this method, a covariant form of the M1 closure scheme is used to close the radiation moments, and the radiative source terms are treated semi-implicitly in order to handle both optically thin and optically thick regimes. The scheme has been implemented in a conservative general relativistic radiation hydrodynamics code KORAL. The robustness of the code is demonstrated on a number of test problems, including radiative relativistic shock tubes, static radiation pressure supported atmosphere, shadows, beams of light in curved space-time and radiative Bondi accretion. The advantages of M1 closure relative to other approaches such as the Eddington closure and flux-limited diffusion are discussed, and its limitations are also highlighted.},
	language = {en},
	number = {4},
	urldate = {2019-05-28},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Sądowski, Aleksander and Narayan, Ramesh and Tchekhovskoy, Alexander and Zhu, Yucong},
	month = mar,
	year = {2013},
	pages = {3533},
	file = {Full Text PDF:/var/home/bprather/Zotero/storage/2BZF4Y37/Sądowski et al. - 2013 - Semi-implicit scheme for treating radiation under .pdf:application/pdf;Snapshot:/var/home/bprather/Zotero/storage/AXRD2TW5/abstract.html:text/html},
}

@article{sadowski_numerical_2014,
	title = {Numerical simulations of super-critical black hole accretion flows in general relativity},
	volume = {439},
	url = {https://ui.adsabs.harvard.edu/abs/2014MNRAS.439..503S/abstract},
	doi = {10.1093/mnras/stt2479},
	abstract = {A new general relativistic radiation magnetohydrodynamical code KORAL is described, which employs the M1 scheme to close the radiation moment equations. The code has been successfully verified against a number of tests. Axisymmetric simulations of super-critical magnetized accretion on non-rotating (a{\textless}SUB{\textgreater}*{\textless}/SUB{\textgreater} = 0.0) and spinning (a{\textless}SUB{\textgreater}*{\textless}/SUB{\textgreater} = 0.9) black holes are presented. The accretion rates in the two models are dot\{M\}≈ 100-200 dot\{M\}\_Edd. These first general relativistic simulations of super-critical black hole accretion are potentially relevant to tidal disruption events and hyper-accreting supermassive black holes in the early Universe. Both simulated models are optically and geometrically thick, and have funnels through which energy escapes in the form of relativistic gas, Poynting flux and radiative flux. The jet is significantly more powerful in the a{\textless}SUB{\textgreater}*{\textless}/SUB{\textgreater} = 0.9 run. The net energy outflow rate in the two runs correspond to efficiencies of 5 per cent (a{\textless}SUB{\textgreater}*{\textless}/SUB{\textgreater} = 0) and 33 per cent (a{\textless}SUB{\textgreater}*{\textless}/SUB{\textgreater} = 0.9), as measured with respect to the mass accretion rate at the black hole. These efficiencies agree well with those measured in previous simulations of non-radiative geometrically thick discs. Furthermore, in the a{\textless}SUB{\textgreater}*{\textless}/SUB{\textgreater} = 0.9 run, the outflow power appears to originate in the spinning black hole, suggesting that the associated physics is again similar in non-radiative and super-critical accretion flows. While the two simulations are efficient in terms of total energy outflow, both runs are radiatively inefficient. Their luminosities are only ̃1-10L{\textless}SUB{\textgreater}Edd{\textless}/SUB{\textgreater}, which corresponds to a radiative efficiency ̃0.1 per cent. Interestingly, most of the radiative luminosity emerges through the funnels where the local radiative flux is highly super-Eddington.},
	language = {en},
	number = {1},
	urldate = {2019-05-28},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Sądowski, Aleksander and Narayan, Ramesh and McKinney, Jonathan C. and Tchekhovskoy, Alexander},
	month = mar,
	year = {2014},
	pages = {503},
	file = {Full Text PDF:/var/home/bprather/Zotero/storage/9SYJU2KW/Sądowski et al. - 2014 - Numerical simulations of super-critical black hole.pdf:application/pdf;Snapshot:/var/home/bprather/Zotero/storage/N8KTUZUJ/abstract.html:text/html},
}

