@article{abramowicz_foundations_2013,
	title = {Foundations of {Black} {Hole} {Accretion} {Disk} {Theory}},
	volume = {16},
	issn = {1433-8351},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5256006/},
	doi = {10.12942/lrr-2013-1},
	abstract = {This review covers the main aspects of black hole accretion disk theory. We begin with the view that one of the main goals of the theory is to better understand the nature of black holes themselves. In this light we discuss how accretion disks might reveal some of the unique signatures of strong gravity: the event horizon, the innermost stable circular orbit, and the ergosphere. We then review, from a first-principles perspective, the physical processes at play in accretion disks. This leads us to the four primary accretion disk models that we review: Polish doughnuts (thick disks), Shakura-Sunyaev (thin) disks, slim disks, and advection-dominated accretion flows (ADAFs). After presenting the models we discuss issues of stability, oscillations, and jets. Following our review of the analytic work, we take a parallel approach in reviewing numerical studies of black hole accretion disks. We finish with a few select applications that highlight particular astrophysical applications: measurements of black hole mass and spin, black hole vs. neutron star accretion disks, black hole accretion disk spectral states, and quasi-periodic oscillations (QPOs).},
	number = {1},
	urldate = {2019-06-13},
	journal = {Living Reviews in Relativity},
	author = {Abramowicz, Marek A. and Fragile, P. Chris},
	year = {2013},
	pmid = {28179840},
	pmcid = {PMC5256006},
	file = {PubMed Central Full Text PDF:/var/home/bprather/Zotero/storage/2X87UV76/Abramowicz and Fragile - 2013 - Foundations of Black Hole Accretion Disk Theory.pdf:application/pdf},
}

